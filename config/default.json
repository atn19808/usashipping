{
  "shop": {
    "homeUrl": "https://quaxave.com"
  },
  "system": {
    "theme": "usashipping",
    "extensions": [
      {
        "name": "quaxave_custom_login",
        "resolve": "extensions/quaxave_custom_login",
        "enabled": true,
        "priority": 10
      },
      {
        "name": "google_login",
        "resolve": "node_modules/@evershop/google_login",
        "enabled": true,
        "priority": 20
      },
      {
        "name": "quaxave_custom_product_view",
        "resolve": "extensions/quaxave_custom_product_view",
        "enabled": true,
        "priority": 30
      },
      {
        "name": "quaxave_custom_address",
        "resolve": "extensions/quaxave_custom_address",
        "enabled": true,
        "priority": 50
      },
      {
        "name": "quaxave_fetch_pricing",
        "resolve": "extensions/quaxave_fetch_pricing",
        "enabled": true,
        "priority": 60
      },
      {
        "name": "quaxave_cart_total_in_vnd",
        "resolve": "extensions/quaxave_cart_total_in_vnd",
        "enabled": true,
        "priority": 70
      }
    ],
    "jobs": [
      {
        "name": "fetchRate",
        "resolve": "@evershop/../../extensions/quaxave_fetch_pricing/jobs/fetchRate.js",
        "schedule": "0 */12 * * *",
        "enabled": true
      }
    ]
  },
  "themeConfig": {
    "logo": {
      "alt": "Qua Xa Ve",
      "src": "/images/logo-full.png",
      "width": 100,
      "height": 100
    },
    "headTags": {
      "links": [
        {
          "rel": "icon",
          "href": "/favicon.ico"
        }
      ]
    },
    "copyRight": "Â© 2025 Qua Xa Ve. All rights reserved."
  },
  "customer": {
    "addressSchema": {
      "type": "object",
      "properties": {
        "sender_full_name": {
          "type": "string"
        },
        "sender_telephone": {
          "type": ["string", "number"]
        },
        "full_name": {
          "type": "string"
        },
        "telephone": {
          "type": ["string", "number"]
        },
        "address_1": {
            "type": "string"
        },
        "address_2": {
            "type": "string"
        },
        "city": {
            "type": "string"
        },
        "province": {
            "type": "string"
        },
        "country": {
            "type": "string",
            "pattern": "^[A-Z]{2}$"
        },
        "postcode": {
            "type": ["string", "number"]
        }
      },
      "required": [
        "full_name",
        "telephone",
        "address_1",
        "city",
        "country",
        "province",
        "postcode"
      ],
      "additionalProperties": true
    }
  }
}